<!DOCTYPE html>
<html lang="pt-br">
<head>
    <script>
        window.FontAwesomeConfig = {
            searchPseudoElements: true
        }
    </script>

    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  
    <title>Adilson Lustoza - Analista de Sistemas</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">


    <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico" />
    <link href="~/font-awesome/css/all.min.css" rel="stylesheet" />

    <link href="https://fonts.googleapis.com/css?family=Roboto&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans|Roboto&display=swap" rel="stylesheet">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>

    <!--Bibliotecas JavaScript-->
    @*@Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/modernizr")*@
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery.maskedinput/1.4.1/jquery.maskedinput.min.js"></script>
    <!--Identidade Visual-->
    @*@Scripts.Render("~/bundles/bootstrap")
    @Styles.Render("~/Content/css")*@

</head>
<body>

    <partial name="_MenuTop" />
    @*@{
            Html.RenderPartial("_MenuTop");
        }*@

    <div class="container-fluid layout_main">

        @if (TempData["Message"] != null)
        {

            <script type="text/javascript">
                $(function () {
                    $.toaster({ priority: 'info', title: 'Mensagem', message: '@TempData["Message"].ToString()' });
                });
            </script>


        }

        @RenderBody()

    </div>

    <partial name="_Footer" />
    @*@{Html.RenderPartial("_Footer"); }*@

    @*@Scripts.Render("~/bundles/own")
        @Scripts.Render("~/bundles/pluggins")*@


    @RenderSection("scripts", required: false)


</body>
</html>


